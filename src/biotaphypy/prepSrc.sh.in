#!/bin/bash

# Purpose: Fetch Biotaphy OpenTree of Life service wrapper package
OTOL_WRAPPER_URL=https://github.com/biotaphy/@ARCHIVENAME@/archive
OTOL_WRAPPER_PKG=v@VERSION@.@TARBALL_POSTFIX@

get_package () {
  echo "Fetch OTOL wrapper package"
  wget "$OTOL_WRAPPER_URL/$OTOL_WRAPPER_PKG"
}

### main ###
get_package