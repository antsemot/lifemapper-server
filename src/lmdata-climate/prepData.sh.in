#!/bin/bash

# Purpose: Fetch climate data named in CLIMATE_DATA  

create_climate_data () {
  echo "Fetch climate data"
  LMURL="http://lifemapper.org/dl"
  curl -L "$LMURL/@CLIMATE_DATA@.@TARBALL_POSTFIX@" -o ../lmdata-climate/@CLIMATE_DATA@.@TARBALL_POSTFIX@
  done 
}

### main ###
create_climate_data
