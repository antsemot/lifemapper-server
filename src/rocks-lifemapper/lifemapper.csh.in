# lifemapper-server roll

set PYBIN=/opt/python/bin
set LMBIN=@PKGROOT@/bin
set PGSQLBIN=/usr/pgsql-@PGSQLVER@/bin

set path = ( $PGSQLBIN $PYBIN $LMBIN $path )
setenv PYTHON /opt/python/bin/@PYTHONVER@

setenv PYTHON_EGG_CACHE @LMWEB@/.python-eggs
setenv PYTHONPATH "@LMHOME@:@LMHOME@/LmClient/LmPython/lifemapper:@PKGROOT@"

setenv PGHOST /var/run/postgresql
setenv LIFEMAPPER_CONFIG_FILE @LMHOME@/config/config.ini

