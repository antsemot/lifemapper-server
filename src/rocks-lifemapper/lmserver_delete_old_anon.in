#!/bin/bash

# Set environment in case it is not already set
. /etc/profile.d/lifemapper.sh

# load opt-python
bash /usr/share/Modules/init/bash
module load opt-python
export PYTHONPATH=@LMHOME@

# Log latest results of this job
LOG=/tmp/`/bin/basename $0`.log
touch $LOG

@PYBIN@ @LMHOME@/LmDbServer/tools/deleteOldAnonymous.py  > $LOG 2>&1
